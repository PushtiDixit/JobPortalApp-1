<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.jobportal1">

    <uses-permission android:name="android.permission." />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Job Portal1"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EmployeeHomepage"></activity>
        <activity android:name=".Homepage" />
        <activity android:name=".HaveAnAccount" />
        <activity
            android:name=".Main1"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Login" />
        <activity android:name=".Employer" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CalenderActivity" />
    </application>

</manifest>